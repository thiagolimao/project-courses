{
    "jquery": {
        "src": [
            "./node_modules/jquery/dist/jquery.min.js",
            "./node_modules/jquery/dist/jquery.min.map"
        ],
        "dest": "assets/vendor/jquery/",
        "js_link": "assets/vendor/jquery/jquery.min.js"
    },
    "jquery.easing": {
        "src": "./node_modules/jquery.easing/**/*",
        "dest": "assets/vendor/jquery.easing/",
        "js_link": "assets/vendor/jquery.easing/jquery.easing.min.js"
    },
    "bootstrap": {
        "src": "./node_modules/bootstrap/dist/**/*",
        "dest": "assets/vendor/bootstrap/",
        "css_link": "assets/vendor/bootstrap/css/bootstrap.min.css",
        "js_link": "assets/vendor/bootstrap/js/bootstrap.min.js"
    },
    "aos": {
        "src": "./node_modules/aos/dist/**/*",
        "dest": "assets/vendor/aos/",
        "css_link": "assets/vendor/aos/aos.css",
        "js_link": "assets/vendor/aos/aos.js"
    },
    "select2": {
        "src": "./node_modules/select2/dist/**/*",
        "dest": "assets/vendor/select2/",
        "css_link": "assets/vendor/select2/css/select2.min.css",
        "js_link": "assets/vendor/select2/js/select2.min.js"
    },
    "font-awesome": {
        "srcs": [
            {
                "src": "./node_modules/font-awesome/css/*",
                "dest": "assets/vendor/font-awesome/css/"
            },
            {
                "src": "./node_modules/font-awesome/fonts/*",
                "dest": "assets/vendor/font-awesome/fonts/"
            }
        ],
        "css_link": "assets/vendor/font-awesome/css/font-awesome.min.css"
    }
}